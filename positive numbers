list1=[12,-7,5,64,-14]
list2=[12,14,-95,3]
list=[]
a=[]
k=0
print("The positive number in list1 and list2 are....")
for i in list1:
  if i>0:
    a.append(i)
for i in a:
  for j in range(0,len(a)-1):
    print(a[j],end=",")
  break
print(a[len(a)-1])
print("\n")
for i in list2:
  if i>0:
    list.append(i)
print(list)
